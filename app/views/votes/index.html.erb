<h1>Listing votes</h1>

<table>
  <tr>
    <th>Article</th>
    <th>Result</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= vote.article.title %></td>
    <td><%= vote.result %></td>
    <td><%= link_to 'Show', user_vote_path(current_user, vote) %></td>
    <td><%= link_to 'Edit', edit_user_vote_path(current_user, vote) %></td>
    <td><%= link_to 'Destroy', user_vote_path(current_user, vote), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote', new_user_vote_path %>
